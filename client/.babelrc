{
  "presets": [
    "@babel/react",
    "@babel/preset-typescript",
    ["@babel/env", { "modules": "auto" }]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [".ts", ".tsx", ".js", ".jsx"],
        "root": ["./src"],
        "alias": {
          "root": "",
          "src": "/src"
        }
      }
    ]
  ]
}
